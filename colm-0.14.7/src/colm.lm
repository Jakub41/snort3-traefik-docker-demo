|	[DOLLAR code_factor] :Dollar
|	[DOLLAR DOLLAR code_factor] :DollarDollar
|	[CARET code_factor] :Caret
|	[AT code_factor] :At
|	[PERCENT code_factor] :Percent
|	[code_factor] :Base

def opt_eos
