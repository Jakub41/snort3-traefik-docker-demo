|	[red_any]
|	[RED_LHS]
|	[RED_RHS_REF]
|	[RED_TREE_REF]
|	[RED_RHS_LOC]
|	[RED_RHS_NREF]
|	[RED_TREE_NREF]
|	[RED_RHS_NLOC]
|	[RED_OPEN HostItems: host_item<* RED_CLOSE]

