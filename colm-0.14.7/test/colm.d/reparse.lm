lex
	ignore /space+/
	literal `* `( `)
	token id /[a-zA-Z_]+/
end

def item 
	[id]
|	[`( item* `)]

def start 
	[item*]

parse Input: item*[ stdin ]

S: start = cons start[ Input ]

parse Again: start[ %Input ]

print[ @Again ]

